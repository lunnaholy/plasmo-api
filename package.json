{
    "name": "plasmo-api",
    "version": "0.1.0",
    "description": "A library that provides developers access to the Plasmo API.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "del /q lib && tsc",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lunnaholy/plasmo-api.git"
    },
    "keywords": [
        "plasmo api",
        "api"
    ],
    "author": "lunnaholy",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/lunnaholy/plasmo-api/issues"
    },
    "homepage": "https://github.com/lunnaholy/plasmo-api#readme",
    "devDependencies": {
        "@types/node": "^18.0.0",
        "prettier": "^2.7.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "node-fetch": "2.6.7"
    }
}
